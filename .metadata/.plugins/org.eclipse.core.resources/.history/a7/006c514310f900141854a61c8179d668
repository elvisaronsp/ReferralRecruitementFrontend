var app = angular.module('LGApp.controllers', []);

app.controller('LoginCtrl', [ '$scope', '$location',
		function($scope, $location) {
			$scope.app = function() {
				$location.path('/app');
			};
		},
		function ($scope, $location) {
            $scope.app = function () {
              $location.path('/app');
            };
		}]);