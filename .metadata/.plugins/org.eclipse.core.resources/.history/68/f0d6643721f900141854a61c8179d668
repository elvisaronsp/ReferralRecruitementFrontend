var var_login_ctrl = angular.module('LGApp.controllers', []);

var_login_ctrl.controller('LoginCtrl', [ '$scope', '$location',
		function($scope, $location) {
			$scope.checkCredentials = function() {
				$location.path = '/login';
			};
		},
		function($scope, $location) {
			$scope.trying = function() {
				$location.path = '/error';
			};
		}]);